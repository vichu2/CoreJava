<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
    <bean id ='a1' class="com.simplilearn.mphasis.models.Address">
    	<property name="city" value = "Visakhapatnam"></property>
    	<property name="state" value = "AndhraPradesh"></property>
    	<property name="country" value = "India"></property>
    </bean>
    <bean id='e1' class="com.simplilearn.mphasis.models.Employee" autowire = "byType">
    	<property name="Firstname" value = "Jogi"></property>
   		<property name="Secondname" value = "SaiKiran"></property>
    	
    </bean>
    
    
</beans>